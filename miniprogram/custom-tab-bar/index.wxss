.custom-tab-bar {
  width: 100%;
  height: 176rpx;
  background: #FFF;
  box-shadow: 0px -2rpx 2rpx 0px rgba(0, 0, 0, 0.05);
  border-radius: 48rpx 48rpx 0px 0px;
  position: fixed;
  bottom: 0;
}

.custom-tab-bar .icons-row {
  height: 88rpx;
  margin-top: 24rpx;
  padding: 0 104rpx;
  /* background: #21D493; */
  display: flex;
  flex-direction: row;
  align-items: center;  
  justify-content: space-between;
}

.custom-tab-bar .icons-row .icon-image {
  width: 48rpx;
  height: 48rpx;
}

.custom-tab-bar .icons-row .home {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.custom-tab-bar .icons-row .personal {
  /* margin-right: 70rpx; */
  display: flex;
  flex-direction: column;
  align-items: center;
}

.custom-tab-bar .icons-row .underText {
  height: 14px;
  font-size: 20rpx;
  line-height: 14px;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #CDCDCD;
  position: relative;
  top: 6px;
}

.custom-tab-bar .middle {
  width: 112rpx;
  height: 112rpx;
  background: #000;
  box-shadow: 0px 0px 20rpx 0px rgba(48, 145, 247, 0.1);
  border-radius: 44rpx;
  position: relative;
  bottom: 36rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.custom-tab-bar .middle .cross-row {
  width: 24px;
  height: 6px;
  background: #FFFFFF;
  border-radius: 3px;
  position: absolute;
}

.custom-tab-bar .middle .cross-column {
  width: 6px;
  height: 24px;
  background: #FFFFFF;
  border-radius: 3px;
  position: absolute;
}

